<h1> New Artist </h1>

<%= form_for @artist do |f| %>
    
    <%= f.label :name, "Please put name here" %>
    <%= f.text_field :name %>
    <%= f.label :age %>
    <%= f.number_field :age %>
    <%= f.label :title, "Please put title here" %>
    <%= f.text_field :title %>
    <%= f.submit "Log a new artist" %>

<% end %>

<p> <%= link_to "See All Artists", artists_path %> </p>
